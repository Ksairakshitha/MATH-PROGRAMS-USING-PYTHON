   "source": [
    "\n",
    "#FIND THE GRADIENT OF THE FUNCTION\n",
    "from sympy.vector import*\n",
    "from sympy import symbols\n",
    "N = CoordSys3D('N')\n",
    "x,y,z = symbols('x,y,z')\n",
    "A = N.x**2*N.y*N.z*N.i+N.y**2*N.x*N.z*N.j+N.z**2*N.x*N.y*N.k\n",
    "delop=Del()\n",
    "divA=delop.dot(A).doit()\n",
    "print('Divergence of A =   ',divA)"
   ]
